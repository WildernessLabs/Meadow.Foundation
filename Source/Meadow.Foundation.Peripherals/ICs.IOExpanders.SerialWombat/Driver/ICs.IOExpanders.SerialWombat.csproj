<Project Sdk="Meadow.Sdk/1.1.0">
  <PropertyGroup>
    <Version>1.8.0</Version>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <LangVersion>10.0</LangVersion>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageIcon>icon.png</PackageIcon>
    <Authors>Wilderness Labs, Inc</Authors>
    <TargetFramework>netstandard2.1</TargetFramework>
    <OutputType>Library</OutputType>
    <AssemblyName>SerialWombat</AssemblyName>
    <Company>Wilderness Labs, Inc</Company>
    <PackageProjectUrl>http://developer.wildernesslabs.co/Meadow/Meadow.Foundation/</PackageProjectUrl>
    <PackageId>Meadow.Foundation.ICs.IOExpanders.SerialWombat</PackageId>
    <RepositoryUrl>https://github.com/WildernessLabs/Meadow.Foundation</RepositoryUrl>
    <PackageTags>Meadow.Foundation,IOExpanders,SerialWombat,serial,Multiplexer,wombat,18ab,gpio,pwm,servo</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>SerialWombat I2C IO expander for GPIO, PWM, servos, etc.</Description>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <None Include=".\Readme.md" Pack="true" PackagePath="" />
    <None Include="..\..\..\icon.png" Pack="true" PackagePath="" />
    <PackageReference Include="Meadow.Foundation" Version="1.8.0.1" />
    <PackageReference Include="Meadow.Foundation.Sensors.Distance.Hcsr04" Version="1.8.0.1" />
    <PackageReference Include="Meadow.Foundation.Servos.ServoCore" Version="1.8.0.1" />
  </ItemGroup>
</Project>
